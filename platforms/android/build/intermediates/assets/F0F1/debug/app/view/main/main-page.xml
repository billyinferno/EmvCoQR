<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="EMVCO QR" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <Button text="Scan QR" tap="{{ scanQR }}" class="btn btn-primary btn-active"/>
        <Button text="Generate QR" tap="{{ generateQR }}" class="btn btn-primary btn-active"/>
    </StackLayout>
</Page>
